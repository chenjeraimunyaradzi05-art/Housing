# VÖR Mobile App Environment Variables
# Copy this file to .env and update the values

# ===========================================
# API Configuration
# ===========================================
EXPO_PUBLIC_API_URL=http://localhost:5000
EXPO_PUBLIC_WS_URL=ws://localhost:5000

# ===========================================
# App Configuration
# ===========================================
EXPO_PUBLIC_APP_NAME=VÖR
EXPO_PUBLIC_ENV=development

# ===========================================
# Feature Flags
# ===========================================
EXPO_PUBLIC_ENABLE_CO_INVEST=true
EXPO_PUBLIC_ENABLE_SAFE_HOUSING=true
EXPO_PUBLIC_ENABLE_BIOMETRICS=true

# ===========================================
# Analytics
# ===========================================
EXPO_PUBLIC_MIXPANEL_TOKEN=

# ===========================================
# Error Tracking (Sentry)
# ===========================================
EXPO_PUBLIC_SENTRY_DSN=

# ===========================================
# Maps (Mapbox)
# ===========================================
EXPO_PUBLIC_MAPBOX_TOKEN=

# ===========================================
# Push Notifications (Expo)
# ===========================================
EXPO_PUBLIC_PROJECT_ID=

# ===========================================
# Deep Linking
# ===========================================
EXPO_PUBLIC_SCHEME=vor
